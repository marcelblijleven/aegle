{
  "name": "aegle-server",
  "version": "1.0.0",
  "description": "Server for aegle",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx}"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "setupFiles": [
      "jest-date-mock"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelblijleven/aegle.git"
  },
  "keywords": [
    "aegle",
    "healthcheck",
    "monitoring"
  ],
  "author": "Marcel Blijleven <marcelblijleven@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelblijleven/aegle/issues"
  },
  "homepage": "https://github.com/marcelblijleven/aegle#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-date-mock": "^1.0.8"
  }
}
